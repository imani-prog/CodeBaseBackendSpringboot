      "step4_rate": "POST /api/telemedicine/sessions/{id}/rate?rating=5&feedback=..."
    },
    "emergency_workflow": {
      "step1_create_urgent": "POST /api/telemedicine/sessions (with priority=URGENT)",
      "step2_immediate_start": "POST /api/telemedicine/sessions/{id}/start",
      "step3_complete_or_refer": "POST /api/telemedicine/sessions/{id}/complete OR escalate to physical visit"
    },
    "follow_up_workflow": {
      "step1_check_history": "GET /api/telemedicine/sessions/by-patient/{patientId}",
